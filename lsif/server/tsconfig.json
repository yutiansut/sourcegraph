{
  "extends": "@sourcegraph/tsconfig",
  "compilerOptions": {
    "noEmit": false,
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "out",
    "lib": ["esnext", "esnext.asynciterable"],
    "resolveJsonModule": true,
    "skipLibCheck": true,
  },
  "files": [],
  "include": ["src"],
  "exclude": ["node_modules"],
}
