{
  "name": "lsif-server",
  "description": "LSIF Server with an HTTP API",
  "author": "Microsoft Corporation and Sourcegraph",
  "license": "MIT",
  "version": "0.1.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "serve": "tsc-watch --onSuccess \"node out/http-server.js\" --noClear",
    "build": "parcel build --out-dir out --out-file http-server.bundle.js --target node --bundle-node-modules src/http-server.ts --no-minify",
    "schema": "typescript-json-schema ./node_modules/lsif-protocol/lib/protocol.d.ts '*' -o src/lsif.schema.json --titles --ignoreErrors --noExtraProps --required --strictNullChecks"
  },
  "dependencies": {
    "async-middleware": "^1.2.1",
    "body-parser": "^1.19.0",
    "dgraph-js": "^1.3.1",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "grpc": "^1.22.2",
    "json-schema-ref-parser": "^7.1.0",
    "lodash": "^4.17.15",
    "lsif-protocol": "^0.4.3",
    "map-obj": "^4.1.0",
    "morgan": "^1.9.1",
    "mz": "^2.7.0",
    "relateurl": "^0.2.7",
    "semver": "^6.3.0",
    "tagged-template-noop": "^2.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/express": "4.17.0",
    "@types/google-protobuf": "3.7.1",
    "@types/lodash": "4.14.136",
    "@types/morgan": "1.7.36",
    "@types/mz": "0.0.32",
    "@types/protobufjs": "6.0.0",
    "@types/relateurl": "0.2.28",
    "@types/semver": "6.0.1",
    "parcel-bundler": "^1.12.3",
    "source-map-support": "^0.5.12",
    "tsc-watch": "^2.2.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "typescript-json-schema": "^0.39.0"
  }
}
